{
  "$schema": "https://turbo.build/schema.json",
  "ui": "stream",
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "env": ["AWS_PROFILE", "NODE_CONFIG_ENV", "NODE_ENV"]
    },
    "env": {
      "dependsOn": ["^env"],
      "env": ["AWS_PROFILE", "NODE_CONFIG_ENV"]
    },
    "locale": {
      "dependsOn": ["^locale"]
    },
    "notion": {
      "dependsOn": ["^notion"]
    },
    "generate:api": {
      "dependsOn": ["^generate:api"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "clean": {
      "cache": false
    }
  }
}
